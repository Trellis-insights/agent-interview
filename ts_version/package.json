{
  "name": "temporal-agent-ts",
  "version": "0.1.0",
  "description": "TypeScript version of the temporal-agent project",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server/index.ts",
    "start": "node dist/server/index.js",
    "worker": "tsx src/temporal/worker.ts",
    "worker:build": "node dist/temporal/worker.js",
    "temporal:worker": "tsx src/temporal/worker.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "start-all": "./scripts/start-all.sh",
    "start-all:docker": "./scripts/start-all.sh docker",
    "start-all:local": "./scripts/start-all.sh local",
    "start-local-only": "./scripts/start-local-only.sh",
    "start-local-temporal": "AUTO_TEMPORAL=true ./scripts/start-local-only.sh",
    "start-local-mock": "MOCK_SERVICES=true ./scripts/start-local-only.sh",
    "stop-all": "./scripts/stop-all.sh",
    "stop-force": "./scripts/stop-all.sh force",
    "services:status": "./scripts/stop-all.sh status",
    "services:health": "./scripts/start-all.sh health"
  },
  "keywords": [
    "temporal",
    "agent",
    "typescript",
    "workflow",
    "ai"
  ],
  "author": "Vikram Khandelwal <vikram@runtrellis.com>",
  "license": "MIT",
  "dependencies": {
    "@temporalio/activity": "^1.10.0",
    "@temporalio/client": "^1.10.0",
    "@temporalio/common": "^1.13.0",
    "@temporalio/worker": "^1.10.0",
    "@temporalio/workflow": "^1.10.0",
    "@trpc/server": "^10.45.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@temporalio/testing": "^1.13.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
